{
   "action": {
      "default_icon": "src/assets/img/logo.png",
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.bundle.js"
   },
   "content_scripts": [ {
      "css": [ "content.styles.css" ],
      "js": [ "contentScript.bundle.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "Optimize your LinkedIn profile to get more recruiters in your inbox, appear in 2.5X more search results, and impress your network!",
   "externally_connectable": {
      "matches": [ "https://auth-extension.web.app/*", "https://extension-auth.web.app/*", "https://job-tracker-test-app.web.app/*", "https://dev-auth-extension.web.app/*" ]
   },
   "icons": {
      "128": "src/assets/img/logo.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Careerflow LinkedIn Optimization",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.10",
   "web_accessible_resources": [ {
      "matches": [ "https://*/*" ],
      "resources": [ "content.styles.css", "src/assets/*" ]
   } ]
}
